msgctxt "amazon.description"
msgid "Searches <a href=\"http://www.amazon.com/\">Amazon.com</a> for items matching your words."
msgstr "Cerca a l'<a href=\"http://www.amazon.com/\">Amazon.com</a> articles que coincideixen amb les vostres paraules."

msgctxt "amazon.preview"
msgid "Searching ${AmazLink} for items matching <b>${query}</b>."
msgstr "Cerca a l'${AmazLink} articles que coincideixen amb <b>${query}</b>."

msgctxt "amazon.preview"
msgid "Error searching ${AmazLink}."
msgstr "S'ha produït un error en cercar a ${AmazLink}."

msgctxt "amazon.preview"
msgid "Found <b>${numitems}</b> item{if numitems - 1}s{/if} on <a href='http://www.amazon.com/'>Amazon.com</a> matching <b>${query}</b>."
msgstr "S'ha trobat <b>${numitems}</b> article{if numitems - 1}s{/if} a <a href='http://www.amazon.com/'>Amazon.com</a> que coincideixen amb <b>${query}</b>."

#. VideoSurf command:
#. use | to separate multiple name values:
msgctxt "VideoSurf.names"
msgid "VideoSurf|video|videos"
msgstr "VideoSurf|vídeo|vídeos"

msgctxt "VideoSurf.help"
msgid "Please email us at feedback@videosurf.com if you have any trouble or feature ideas!"
msgstr "Envieu un missatge a feedback@videosurf.com si teniu cap problema o idees per implementar."

msgctxt "VideoSurf.description"
msgid "Performs a VideoSurf video search with advanced preview."
msgstr "Realitza una cerca al VideoSurf amb previsualització avançada."

msgctxt "VideoSurf.preview"
msgid "Loading videos..."
msgstr "S'estan carregant els vídeos…"

#. YouTube command:
#. use | to separate multiple name values:
msgctxt "YouTube.names"
msgid "YouTube"
msgstr "YouTube"

msgctxt "YouTube.description"
msgid "Searches <a href=\"http://www.youtube.com\">YouTube</a> for videos matching your words. Previews the top results."
msgstr "Cerca al <a href=\"http://www.youtube.com\">YouTube</a> els vídeos que coincideixen amb les vostres paraules. Es previsualitzen els primers resultats."

#. Flickr command:
#. use | to separate multiple name values:
msgctxt "Flickr.names"
msgid "Flickr|images"
msgstr "Flickr|imatges"

msgctxt "Flickr.description"
msgid "Searches <a href=\"http://www.flickr.com\">Flickr</a> for pictures matching your keywords. Previews the top pictures."
msgstr "Cerca al <a href=\"http://www.flickr.com\">Flickr</a> les imatges que coincideixen amb les vostres paraules . Es previsualitzen les primeres imatges."

msgctxt "Flickr.preview"
msgid "Searches for photos on Flickr."
msgstr "Cerca de fotos al Flickr."

msgctxt "Flickr.preview"
msgid "Searching for photos on Flickr..."
msgstr "S'estan cercant fotos al Flickr…"

msgctxt "Flickr.preview"
msgid "Error searching Flickr"
msgstr "S'ha produït un error en cercar al Flickr"

msgctxt "Flickr.preview"
msgid "Error searching Flickr."
msgstr "S'ha produït un error en cercar al Flickr."

msgctxt "Flickr.preview"
msgid "${nummatches} photos were found on Flickr."
msgstr "S'han trobat ${nummatches} al Flickr."

#. Bugzilla command:
#. use | to separate multiple name values:
msgctxt "Bugzilla.names"
msgid "Bugzilla"
msgstr "Bugzilla"

msgctxt "Bugzilla.description"
msgid "Searches <a href=\"http://bugzilla.mozilla.com\">Bugzilla</a> for Mozilla bugs matching the given words."
msgstr "Cerca al <a href=\"http://bugzilla.mozilla.com\">Bugzilla</a> els informes d'error de Mozilla que coincideixen amb les paraules donades."

#. msn command:
#. use | to separate multiple name values:
msgctxt "msn.names"
msgid "msn"
msgstr "msn"

msgctxt "msn.description"
msgid "Searches <a href=\"http://search.msn.com\">MSN</a> for the given words."
msgstr "Cerca a l'<a href=\"http://search.msn.com\">MSN</a> les paraules donades."

msgctxt "msn.preview"
msgid "Searches MSN for ${query}"
msgstr "Cerca ${query} al MSN."

msgctxt "msn.preview"
msgid "Searches MSN for the given words."
msgstr "Cerca al MSN les paraules donades."

#. ebay command:
#. use | to separate multiple name values:
msgctxt "ebay.names"
msgid "ebay"
msgstr "ebay"

msgctxt "ebay.description"
msgid "Searches <a href=\"http://search.ebay.com\">eBay</a> for auctions matching the given words."
msgstr "Cerca a l'<a href=\"http://search.ebay.com\">eBay</a> les subhastes que coincideixen amb les paraules donades."

msgctxt "ebay.preview"
msgid "Searches eBay for ${query}"
msgstr "Cerca ${query} a l'eBay"

msgctxt "ebay.preview"
msgid "Searches eBay for the given words."
msgstr "Cerca a l'eBay les paraules donades."

#. ask command:
#. use | to separate multiple name values:
msgctxt "ask.names"
msgid "ask"
msgstr "ask"

msgctxt "ask.description"
msgid "Searches <a href=\"http://www.ask.com\">Ask.com</a> for the given words."
msgstr "Cerca a l'<a href=\"http://www.ask.com\">Ask.com</a> les paraules donades."

msgctxt "ask.preview"
msgid "Searches Ask.com for ${query}"
msgstr "Cerca ${query} a l'Ask.com"

msgctxt "ask.preview"
msgid "Searches Ask.com for the given words."
msgstr "Cerca a l'Ask.com les paraules donades."

#. answers command:
#. use | to separate multiple name values:
msgctxt "answers.names"
msgid "answers"
msgstr "answers"

msgctxt "answers.description"
msgid "Searches <a href=\"http://www.answers.com\">Answers.com</a> for the given words."
msgstr "Cerca a l'<a href=\"http://www.answers.com\">Answers.com</a> les paraules donades."

#. yelp command:
#. use | to separate multiple name values:
msgctxt "yelp.names"
msgid "yelp"
msgstr "yelp"

msgctxt "yelp.help"
msgid "You can search for restaurants near a certain location using the <i>near</i> modifier.  For example, try &quot;yelp pizza near boston&quot;."
msgstr "Podeu cercar restaurants prop d'una ubicació fent servir el modificador <i>prop de</i>. Per exemple, proveu «yelp pizza prop de Boston»."

msgctxt "yelp.description"
msgid "Searches <a href=\"http://www.yelp.com\">Yelp</a> for restaurants matching your words.  Previews the top results."
msgstr "Cerca al <a href=\"http://www.yelp.com\">Yelp</a> restaurants que coincideixen amb les vostres paraules.  Es previsualitzar els primers resultats."

msgctxt "yelp.preview"
msgid "Searches yelp for restaurants in your area"
msgstr "Cerca al yelp els restaurants en la vostra àrea."

msgctxt "yelp.execute"
msgid "${name} is a ${stars} restaurant in{if defined('where')} <a href='${whereUrl}'>${where},</a>{/if} ${city}."
msgstr "${name} és un restaurant de ${stars} a {if defined('where')} <a href='${whereUrl}'>${where},</a>{/if} ${city}."

msgctxt "yelp.execute"
msgid "It's been reviewed ${times} times."
msgstr "S'ha revisat ${times} vegades."

#. check weather command:
#. use | to separate multiple name values:
msgctxt "check weather.names"
msgid "check weather|get weather|weather"
msgstr "consulta l'oratge|consulta el temps|oratge|temps|meteo"

msgctxt "check weather.help"
msgid "Try issuing &quot;weather near chicago&quot;.  It works with zip-codes, too.  You can add 'in celsius' or 'in farenheit' to specify the temperature scale."
msgstr "Proveu-ho amb «oratge prop d'Alacant». Pot utilitzar-se 'en cèlsius' o 'en farenheit' per a especificar-ne l'escala de temperatura."

msgctxt "check weather.description"
msgid "Checks the weather for a given location. It detects your current location as a default."
msgstr "Consulta l'oratge en una ubicació determinada. Detecta la vostra ubicació actual per defecte."

msgctxt "check weather.preview"
msgid "Gets the weather for a zip code/city."
msgstr "Aconsegueix un informe de l'oratge prenent el nom d'una ciutat."

msgctxt "check weather.preview"
msgid "Weather for ${location}"
msgstr "L'oratge a ${location}"

#. define command:
#. use | to separate multiple name values:
msgctxt "define.names"
msgid "define|get definition"
msgstr "definició en anglès|defineix en anglès|definix en anglés"

msgctxt "define.help"
msgid "Try issuing &quot;define aglet&quot;"
msgstr "Proveu «defineix en anglès aglet»"

msgctxt "define.description"
msgid "Gives the meaning of a word, using answers.com."
msgstr "Dóna el significat en anglès d'una paraula fent servir answers.com."

msgctxt "define.preview"
msgid "Gives the definition of a word."
msgstr "Dóna el significat en anglès d'una paraula."

msgctxt "define.preview"
msgid "Gives the definition of the word ${word}."
msgstr "Dóna el significat en anglès de la paraula ${word}."

#. google image search command:
#. use | to separate multiple name values:
msgctxt "google image search.names"
msgid "google image search|get images"
msgstr "cerca imatges amb google|aconsegueix imatges"

msgctxt "google image search.help"
msgid "Press return to show the Google Images results, left click on a picture to watch it, right click to insert it in a text or rich text area"
msgstr "Premeu Retorn per a mostrar els resultats de Google Images, boto esquerre sobre una imatge per a visualitzar-la, i botó dret per a inserir-la en un text o en una àrea de text enriquit."

msgctxt "google image search.description"
msgid "Browse and embed pictures from Google Images"
msgstr "Navega i incrusta imatges de Google Images"

msgctxt "google image search.preview"
msgid "Search on Google Images."
msgstr "Cerca en el Google Images."

msgctxt "google image search.preview"
msgid "Searching on Google Images..."
msgstr "S'està cercant en el Google Images…"

msgctxt "google image search.preview"
msgid "Connection error."
msgstr "Error de connexió."

msgctxt "google image search.preview"
msgid "Server error"
msgstr "Error del servidor"

msgctxt "google image search.preview"
msgid "Sorry, this function can be used in an editable box only"
msgstr "Ho sentim, la funció pot utilitzar-se només en un quadre que es pot editar."

msgctxt "google image search.preview"
msgid "Image Search results for ${query}"
msgstr "Resultats de la cerca d'imatges de ${query}"

msgctxt "google image search.preview"
msgid "Sorry, there are no results"
msgstr "Ho sentim, no hi ha cap resultat."

