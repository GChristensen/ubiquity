msgid "count words|word count"
msgstr ""
"compta les paraules|compta els mots|recompte de mots|recompte de paraules"

msgctxt "count words.description"
msgid "Displays the number of words in a selection."
msgstr "Mostra el nombre de paraules d'una selecció"

msgctxt "count words.preview"
msgid "<b>${num}</b> words"
msgstr "<b>${num}</b> paraules"

msgctxt "count words.preview"
msgid "Displays the number of words in a selection."
msgstr "Mostra el nombre de paraules d'una selecció."

msgctxt "count words.execute"
