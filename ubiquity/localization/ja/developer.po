"    <b>Left-click / Enter</b>\n"
"  </dt>\n"
"  <dd>Copy and Quit</dd>\n"
"  <dt>\n"
"    <b>\n"
"      Middle-click /\n"
"      <u>C</u>\n"
"    </b>\n"
"  </dt>\n"
"  <dd>Copy</dd>\n"
"  <dt>\n"
"    <b>\n"
"      Right-click / Esc /\n"
"      <u>Q</u>\n"
"    </b>\n"
"  </dt>\n"
